# RUN:
cargo run -p ylc -- %s -o=out.o
gcc out.o -o a.exe
./a.exe
# IN:

define i32 @main() {
  entry:
    %0 = f32 5.2
    switch f32 %0, default default [
        f32 5.1, is5dot1
        f32 5.2, is5dot2
        f32 5.3, is5dot3
    ]

  default:
    ret i32 0

  is5dot1:
    ret i32 1

  is5dot2:
    ret i32 2

  is5dot3:
    ret i32 3
}

# EXIT_CODE=2