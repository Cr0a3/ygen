# RUN: 
cargo run -p ylc -- -in=%s -o=out.o
gcc out.o -o a.exe
./a.exe
# IN:
define u8 @is_smaller_or_equal(u32 %0, u32 %1) {
  entry:
    %2 = cmp lte u32 %0, %1
    ret u8 %2
}

define u8 @main() {
  entry:
    %0 = u32 5
    %1 = u32 5
    %2 = call u8 is_smaller_or_equal u32 %0 u32 %1

    %3 = u32 1
    %4 = u32 4

    %5 = call u8 is_smaller_or_equal u32 %3 u32 %4

    %6 = add u8 %2, %5

    ret u8 %6
}

# EXIT_CODE=2