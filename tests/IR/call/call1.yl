# RUN: 
cargo run -p ylc -- %s -o=out.o
gcc out.o -o a.exe
./a.exe
# IN:

define u32 @func(u32 %0, u32 %1) {
  entry:
    %2 = add u32 %0, %1
    %3 = sub u32 %2, 5
    br cond %3 isnt0, is0
  
  isnt0:
    ret u32 4

  is0:
    ret u32 0
}
define u32 @main() {
 entry:
    %1 = u32 9
    %2 = u32 21

    %3 = call u32 @func u32 %1 u32 %2

    %4 = add u32 %3, 6

    ret u32 %4
}

# EXIT_CODE=10