# RUN:
cargo run -p ylc -- -in=%s -o=out.o -O
gcc out.o -o a.exe
./a.exe

# IN:

define i32 @main() {
  entry:
	%1 = alloca i32
	%2 = add i32 8, 32
	store i32 %2, %1

	%3 = load i32 %1
    %4 = sub i32 %3, 32 

    %5 = i32 8

    %6 = cmp eq i32 %4, %5

    br cond %6 eq, ne

  eq:
    ret i32 1

  ne:
    ret i32 0
}

# EXIT_CODE=1